<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.DishesDeleteDao">
  <delete id="dishesDelete" parameterType="java.lang.String">
    DELETE FROM
    DISHES
    WHERE
    DISHESNAME = #{dishesName}
  </delete>
</mapper>