<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.DishesAddDao">
  <insert id="dishesAdd"  parameterType="com.pojo.Dishes">
      INSERT INTO
      DISHES(dishesName,dishesPrice,dishesDescription,pictureUrl)
      VALUES
      (#{dishesName},#{dishesPrice},#{dishesDescription},#{pictureUrl})
  </insert>
</mapper>